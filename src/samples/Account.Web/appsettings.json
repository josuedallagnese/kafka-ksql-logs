{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",

    "RequestLogging": {
        "ApplicationName": "account-web",
        "IgnoreStatusCodes": [ 400, 401, 403 ],
        "Mode": "Batch",
        "Kafka": {
            "BootstrapServers": "localhost:9092",
            "Acks": "None",
            "EnableDeliveryReports": false,
            "LingerMs": 10000
        }
    }
}
